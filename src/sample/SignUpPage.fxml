<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #1634;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUpPage">
   <children>
      <TextField fx:id="name" alignment="TOP_CENTER" blendMode="HARD_LIGHT" layoutX="39.0" layoutY="88.0" promptText="Name" />
      <TextField fx:id="lastName" alignment="TOP_CENTER" blendMode="HARD_LIGHT" cacheShape="false" centerShape="false" focusTraversable="false" layoutX="39.0" layoutY="161.0" promptText="Last Name" scaleShape="false">
         <cursor>
            <Cursor fx:constant="NW_RESIZE" />
         </cursor>
      </TextField>
      <TextField fx:id="username" alignment="TOP_CENTER" blendMode="HARD_LIGHT" layoutX="367.0" layoutY="88.0" promptText="Username" />
