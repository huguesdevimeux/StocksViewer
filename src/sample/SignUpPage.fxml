      <Button cache="true" cacheHint="SCALE" depthTest="ENABLE" layoutX="258.0" layoutY="312.0" prefHeight="26.0" prefWidth="83.0" text="Sign Up!" />
      <PasswordField fx:id="passwordField" alignment="TOP_CENTER" blendMode="HARD_LIGHT" layoutX="367.0" layoutY="161.0" promptText="Password" />
      <Label fx:id="wrongPasswordLabel" alignment="TOP_CENTER" layoutX="383.0" layoutY="200.0" prefHeight="18.0" prefWidth="112.0" text="Label" />
      <Label fx:id="wrongSignUpLabel" layoutX="263.0" layoutY="346.0" prefHeight="18.0" prefWidth="83.0" textFill="#43b232">
